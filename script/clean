#!/bin/bash
cd "$(dirname "$0")/.." || exit 111

[[ -d ./node_modules/ ]] && rm -rf ./node_modules/
[[ -d ./dev/ ]] && rm -rf ./dev/
[[ -d ./dist/ ]] && rm -rf ./dist/
[[ -d ./coverage/ ]] && rm -rf ./coverage/
[[ -d ./local-storage-scratch/ ]] && rm -rf ./local-storage-scratch/
[[ -f ./yarn.lock ]] && rm -f yarn.lock

yarn cache clean

echo "Cleaned"
