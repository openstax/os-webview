{
  "name": "@openstax/os-webview",
  "version": "2.0.0",
  "description": "OpenStax webview",
  "scripts": {
    "start": "gulp dev",
    "build": "gulp",
    "upgrade": "jspm update",
    "pretest": "npm run-script build",
    "test": "jest --coverage ./test/src/*/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openstax/os-webview.git"
  },
  "license": "AGPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-jest": "19.0.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-runtime": "^6.20.0",
    "browser-sync": "2.18.8",
    "connect-history-api-fallback": "1.3.0",
    "del": "2.2.2",
    "eslint": "3.19.0",
    "find-pkg": "0.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "3.1.1",
    "gulp-babel": "6.1.2",
    "gulp-cli": "^1.2.2",
    "gulp-clip-empty-files": "0.1.2",
    "gulp-cssnano": "2.1.2",
    "gulp-env": "0.4.0",
    "gulp-eslint": "3.0.1",
    "gulp-htmlmin": "3.0.0",
    "gulp-humans": "2.0.2",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "3.2.0",
    "gulp-jest": "^1.0.0",
    "gulp-load-plugins": "1.5.0",
    "gulp-real-favicon": "0.2.2",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-sass": "3.1.0",
    "gulp-scss-lint": "0.4.0",
    "gulp-sourcemaps": "2.6.0",
    "gulp-superviewsjs": "github:dak/gulp-superviewsjs",
    "gulp-uglify": "2.1.2",
    "gulp-util": "3.0.8",
    "gulp-wrap": "0.13.0",
    "jest": "19.0.2",
    "jest-cli": "19.0.2",
    "jsdom": "10.0.0",
    "jsdom-global": "2.1.1",
    "jspm": "0.17.0-beta.23",
    "regenerator-runtime": "^0.10.3",
    "require-dir": "0.3.1",
    "sw-precache": "5.1.0",
    "systemjs-builder": "0.16.4",
    "systemjs-jest": "^1.0.0",
    "yargs": "7.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^~/(.*)": "<rootDir>/dist/app/$1",
      "superb": "<rootDir>/jspm_packages/npm/superb.js@0.2.13/dist/superb.js",
      "settings": "<rootDir>/dist/settings",
      "fetch": "<rootDir>/jspm_packages/npm/whatwg-fetch@1.1.1/fetch.js"
    },
    "setupFiles": [
      "<rootDir>/test/setupFile.js"
    ]
  },
  "jspm": {
    "main": "src/app/main.js",
    "directories": {
      "lib": "src/app"
    },
    "configFiles": {
      "jspm": "src/jspm.config.js",
      "jspm:dev": "src/jspm.dev.js",
      "jspm:browser": "src/jspm.browser.js"
    },
    "dependencies": {
      "babel-polyfill": "npm:babel-polyfill@^6.9.1",
      "fetch": "npm:whatwg-fetch@^1.0.0",
      "recordo": "npm:recordo@0.0.6",
      "superb": "npm:superb.js@^0.2.7"
    },
    "devDependencies": {
      "console": "github:jspm/nodelibs-console@^0.2.0-alpha",
      "element-dataset": "npm:element-dataset@^2.2.5",
      "jest": "npm:jest@^19.0.2",
      "punycode": "github:jspm/nodelibs-punycode@^0.2.0-alpha",
      "source-map": "npm:source-map@0.2",
      "uglify-js": "npm:uglify-js@^2.6",
      "uglify-to-browserify": "npm:uglify-to-browserify@1.0"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "bcrypt-pbkdf": "npm:bcrypt-pbkdf@^1.0.0",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "dgram": "github:jspm/nodelibs-dgram@^0.2.0-alpha",
      "dns": "github:jspm/nodelibs-dns@^0.2.0-alpha",
      "ecc-jsbn": "npm:ecc-jsbn@~0.1.1",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "fsevents": "npm:fsevents@^1.0.0",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "incremental-dom": "npm:incremental-dom@^0.5.1",
      "jodid25519": "npm:jodid25519@^1.0.0",
      "jsbn": "npm:jsbn@0.1",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "net": "github:jspm/nodelibs-net@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "querystring": "github:jspm/nodelibs-querystring@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "tls": "github:jspm/nodelibs-tls@^0.2.0-alpha",
      "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha",
      "tweetnacl": "npm:tweetnacl@0.14",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zlib": "github:jspm/nodelibs-zlib@^0.2.0-alpha"
    },
    "overrides": {
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.6.4": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:esprima@2.7.3": {
        "jspmNodeConversion": false
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.17.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.3": {
        "jspmNodeConversion": false,
        "format": "cjs"
      }
    }
  },
  "engines": {
    "node": ">=6.2"
  }
}
